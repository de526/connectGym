<mapper namespace="mapper.MemberMapper"><!-- 이건 xml 잡는거고  -->
	<!-- 이메일 중복검사 -->
	<select id="isEnableEmail" parameterType="String" resultType="int">
		select count(mem_mail) from member_t where mem_mail = #{mem_mail}
	</select>
